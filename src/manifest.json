{
	"manifest_version": 2,
	"name": "__MSG_manifest_name__",
	"version": "0.1.5",
	"applications": {
		"gecko": {
			"id": "{5a898e4e-6b5c-4acf-9e29-4c791d3b5f2e}",
			"strict_min_version": "74.0"
		}
	},
	"default_locale": "en",
	"description": "__MSG_manifest_description__",
	"developer": {
		"name": "_y_s",
		"url": "https://github.com/ys-j/YoutubeLiveChatFlusher"
	},
	"icons": {
		"48": "icon.svg",
		"96": "icon.svg"
	},
	"permissions": ["storage"],
	"content_scripts": [
		{
			"all_frames": true,
			"css": ["content.css"],
			"js": ["content.js"],
			"matches": ["*://www.youtube.com/*"],
			"run_at": "document_end"
		}
	],
	"content_security_policy": "default-src 'self';"
}